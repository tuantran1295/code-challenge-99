{
  "name": "express-typescript-crud",
  "version": "1.0.0",
  "description": "A simple Express CRUD API in TypeScript with SQLite and Prisma.",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/server.ts",
